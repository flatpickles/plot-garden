{
  "name": "vibe-plotter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm sketches:sync && next dev",
    "build": "pnpm sketches:sync && next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
